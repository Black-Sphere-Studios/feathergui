<? version="1.0" encoding="UTF-8"?>
<fg:Layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:fg="featherGUI" xsi:schemaLocation="featherGUI feather.xsd" skin="testskin">
	<Window flags="RESIZABLE" text="Test Window" area="130 100 510 420">
		<Menu>
			<MenuItem text="File">
				<Submenu>
					<MenuItem text="New"/>
					<MenuItem text="Open"/>
					<MenuItem text="Save"/>
					<MenuItem text="Save As..."/>
					<Element flags="IGNORE" area="0 0 100% 0" name="Submenu$seperator" userid="0"/>
					<MenuItem text="Quit"/>
				</Submenu>
			</MenuItem>
			<MenuItem text="Edit">
				<Submenu>
					<MenuItem text="Cut"/>
					<MenuItem text="Copy"/>
					<MenuItem text="Paste"/>
					<Element flags="IGNORE" area="0 0 100% 0" name="Submenu$seperator" userid="0"/>
					<MenuItem text="Transform">
						<Submenu>
							<MenuItem text="Free Transform">
								<Submenu>
									<MenuItem text="Apply"/>
								</Submenu>
							</MenuItem>
							<Element flags="IGNORE" area="0 0 100% 0" name="Submenu$seperator" userid="0"/>
							<MenuItem text="Rotate"/>
							<MenuItem text="Skew"/>
							<MenuItem text="Resize"/>
						</Submenu>
					</MenuItem>
				</Submenu>
			</MenuItem>
			<MenuItem text="Options"/>
			<MenuItem text="Help">
				<Submenu>
					<MenuItem text="About"/>
				</Submenu>
			</MenuItem>
		</Menu>
		<Submenu id="#testcontext">
			<MenuItem text="Change Window Text"/>
		</Submenu>
		<TabControl area="0 20 100% 100%">
			<Tab id="#tabfocus" name="Tab 1" userid="0">
				<Button flags="EXPANDX|EXPANDY" text="Not Pressed" onaction="makepressed"/>
				<Box flags="EXPANDX|TILEY" max-width="100" max-height="-1" margin="2 2 2 2" area="10 160 150 240">
					<Checkbox flags="EXPANDX|EXPANDY" margin="2 2 2 2" text="Check test 1"/>
					<Checkbox flags="EXPANDX|EXPANDY" margin="2 2 2 2" text="Check test 22"/>
					<Checkbox flags="EXPANDX|EXPANDY" margin="2 2 2 2" text="Check test 3"/>
					<Checkbox flags="EXPANDX|EXPANDY" margin="2 2 2 2" text="Check test 4"/>
					<Checkbox flags="EXPANDX|EXPANDY" margin="2 2 2 2" text="Check test 5"/>
				</Box>
				<Radiobutton flags="EXPANDX|EXPANDY" text="Radio Test 1" area="190 130 0 0"/>
				<Radiobutton flags="EXPANDX|EXPANDY" text="Radio Test 2" area="190 160 0 0"/>
				<Radiobutton flags="EXPANDX|EXPANDY" text="Radio Test 3" area="190 190 0 0"/>
				<Slider range="0.0" value="200" area="10 70 150 90" onsetvalue="statelistener"/>
				<Progressbar text="Progress" value="0.3" area="10 100 150 125" id="#progbar"/>
				<Combobox text="combo text" area="190 20 300 50" id="#combo">
					<Text text="test text" area="0 0 100% 20"/>
				</Combobox>
			</Tab>
			<Tab name="Long Tab" userid="0">
				<Textbox flags="WORDWRAP" placeholder="placeholder" area="140 30 210 150"/>
				<Dropdown flags="TILEY" max-width="-1" max-height="60" area="10 200 150 230">
					<Text text="Drop 1" area="0 0 100% 20"/>
					<Text text="Drop 2" area="0 0 100% 20"/>
					<Text flags="EXPANDX|EXPANDY" text="Drop 3"/>
					<Text text="Drop 4" area="0 0 100% 20"/>
				</Dropdown>
				<List flags="EXPANDY|SNAP|TILEY|SELECT|DRAGGABLE" dividercolor="FF000000" value="3" area="-350:1 30 -250:1 0">
					<ListItem area="0 0 100% 20"/>
					<Text text="List 1" area="0 0 100% 20"/>
					<Text text="List 2" area="0 0 100% 20"/>
					<Text text="List 3" area="0 0 100% 20"/>
					<Text text="List 4" area="0 0 100% 20"/>
					<ListItem area="0 0 100% 20"/>
					<Text text="List 5" area="0 0 100% 20"/>
				</List>
				<Treeview area="240 10 340 150" contextmenu="#testcontext">
					<TreeItem contextmenu="#testcontext">
						<Text flags="EXPANDX|EXPANDY" text="Parent Item 1"/>
						<TreeItem contextmenu="#testcontext">
							<Text flags="EXPANDX|EXPANDY" text="Item 4"/>
						</TreeItem>
						<TreeItem contextmenu="#testcontext">
							<Text flags="EXPANDX|EXPANDY" text="Item 5"/>
						</TreeItem>
						<TreeItem>
							<Text flags="EXPANDX|EXPANDY" text="Item 6"/>
						</TreeItem>
					</TreeItem>
					<TreeItem>
						<Text flags="EXPANDX|EXPANDY" text="Parent Item 2"/>
						<TreeItem>
							<Text flags="EXPANDX|EXPANDY" text="Item 6"/>
						</TreeItem>
					</TreeItem>
					<TreeItem>
						<Text flags="EXPANDX|EXPANDY" text="Parent Item 3"/>
					</TreeItem>
				</Treeview>
			</Tab>
			<Tab name="Tab 3" userid="0">
				<Box flags="EXPANDY|IGNOREMARGINEDGEX|IGNOREMARGINEDGEY|TILE" padding="5 5 5 5" area="0 0 100% 0">
					<Resource flags="BACKGROUND|RECT" color="FF999999" uv="3 3 3 3" area="0 0 100% 100%"/>
					<Box flags="EXPANDY|IGNOREMARGINEDGEX|IGNOREMARGINEDGEY|TILE" max-width="90" max-height="-1" min-width="60" min-height="-1" padding="5 5 5 5" area="0 0 100% 0">
						<Resource flags="BACKGROUND|RECT" color="FF00BBBB" uv="3 3 3 3" area="0 0 100% 100%"/>
						<Resource flags="RECT" margin="0 5 0 0" color="FFDDDDDD" uv="3 3 3 3" area="0 0 50 25"/>
						<Resource flags="RECT" margin="10 5 0 0" color="FFDDDDDD" uv="3 3 3 3" area="0 0 30 25"/>
					</Box>
					<Box flags="EXPANDY|IGNOREMARGINEDGEX|IGNOREMARGINEDGEY|TILE" max-width="250" max-height="-1" margin="10 5 0 0" padding="5 5 5 5" area="0 0 100% 0">
						<Resource flags="BACKGROUND|RECT" color="FFBBBBBB" uv="3 3 3 3" area="0 0 100% 100%"/>
						<Resource flags="RECT" margin="10 5 0 0" color="FFDDDDDD" uv="3 3 3 3" area="0 0 30 25"/>
						<Resource flags="RECT" max-width="100" max-height="-1" min-width="60" min-height="-1" margin="10 5 0 0" color="FFDDDDDD" uv="3 3 3 3" area="0 0 100% 25"/>
						<Resource flags="RECT" min-width="20" min-height="-1" margin="10 5 0 0" color="FFDDDDDD" uv="3 3 3 3" area="0 0 100% 25"/>
						<Resource flags="RECT" max-width="100" max-height="-1" min-width="20" min-height="-1" margin="10 5 0 0" color="FFDDDDDD" uv="3 3 3 3" area="0 0 100% 25"/>
					</Box>
				</Box>
			</Tab>
			<Tab name="Tab 4" userid="0">
				<Grid flags="TILEY|DRAGGABLE" range="2" value="2" margin="20 20 20 20" area="0 0 100% 100%">
					<Column name="Column 1" userid="0"/>
					<Column name="Column 2" userid="0"/>
					<Column name="Column 3" userid="0"/>
					<Column name="Column 4" userid="0"/>
					<Column name="Column 5" userid="0"/>
					<GridRow flags="EXPANDX|EXPANDY">
						<Text flags="EXPANDY" text="text 11"/>
						<Text flags="EXPANDY" text="text 12"/>
						<Text flags="EXPANDY" text="text 13"/>
						<Text flags="EXPANDY" text="text 14"/>
					</GridRow>
					<GridRow flags="EXPANDX|EXPANDY">
						<Text flags="EXPANDY" text="text 21"/>
						<Text flags="EXPANDY" text="text 22"/>
						<Text flags="EXPANDY" text="text 23"/>
						<Text flags="EXPANDY" text="text 24"/>
					</GridRow>
					<GridRow flags="EXPANDX|EXPANDY">
						<Text flags="EXPANDY" text="text 31"/>
						<Text flags="EXPANDY" text="text 32"/>
						<Text flags="EXPANDY" text="text 33"/>
						<Text flags="EXPANDY" text="text 34"/>
					</GridRow>
					<GridRow flags="EXPANDX|EXPANDY">
						<Text flags="EXPANDY" text="text 41"/>
						<Text flags="EXPANDY" text="text 42"/>
						<Text flags="EXPANDY" text="text 43"/>
						<Text flags="EXPANDY" text="text 44"/>
					</GridRow>
				</Grid>
			</Tab>
		</TabControl>
	</Window>
</fg:Layout>